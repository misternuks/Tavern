<div class="container m-5">
  <div class="card-product">
    <%= image_tag "http://source.unsplash.com/featured/?face&#{rand(1000)}" %>
    <div class="card-product-infos">
      <h2><%= @dm_profile.user.email %></h2>
      <%= @dm_profile.experience %>
      <p><%= link_to 'Book this DM', new_dm_profile_booking_path(@dm_profile), class: 'btn btn-primary'%></p>
    </div>
  </div>
  <p><%= link_to 'Back', dm_profiles_path, class: 'btn btn-info'  %></p>
</div>
